<title>Detail User</title>
<div class="dtl-user">
  <h3 class="my-3">Detail User</h3>
  <div class="table-responsive mt-3">
    <table class="table">
      <tbody>
        <tr>
          <th scope="row">Username</th>
          <td><%= detail.username %></td>
        </tr>
        <tr>
          <th scope="row">Email</th>
          <td><%= detail.email %></td>
        </tr>
        <tr>
          <th scope="row">First Name</th>
          <td><%= detail.UserGameBiodata.firstName %></td>
        </tr>
        <tr>
          <th scope="row">Last Name</th>
          <td><%= detail.UserGameBiodata.lastName %></td>
        </tr>
        <tr>
          <th scope="row">Phone Number</th>
          <td><%= detail.UserGameBiodata.phoneNumber %></td>
        </tr>
      </tbody>
    </table>
  </div>
  <button type="button" onclick="history.back()" class="btn btn-light">
    <i class="bi-arrow-left"></i>
    Kembali
  </button>
</div>
